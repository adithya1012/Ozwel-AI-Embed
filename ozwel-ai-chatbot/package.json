{
  "name": "ozwel-ai-chatbot",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "Ozwel AI Chatbot - Embeddable Medical AI Assistant",
  "keywords": [
    "medical",
    "ai",
    "chatbot",
    "healthcare",
    "embed",
    "iframe",
    "typescript",
    "vite"
  ],
  "author": "Adithya",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "serve": "vite preview --port 3000 --host",
    "server:dev": "nodemon --exec \"node --loader ts-node/esm\" server/server.ts",
    "server:build": "tsc server/server.ts --outDir dist-server --module es2022 --target es2022 --moduleResolution node",
    "server:start": "node dist-server/server.js",
    "demo": "cd demo && python3 -m http.server 8080",
    "test:integration": "npm run preview & sleep 2 && open http://localhost:4173/demo/"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.3.0",
    "nodemon": "^3.1.10",
    "typescript": "~5.8.3",
    "vite": "^7.1.2"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "morgan": "^1.10.1"
  }
}
