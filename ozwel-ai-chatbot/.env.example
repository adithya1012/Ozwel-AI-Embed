# Ozwel AI Chatbot Configuration
# Copy this file to .env and fill in your values

# Server Configuration
PORT=3000
NODE_ENV=development

# OpenAI API Configuration (optional - can be set in UI)
# OPENAI_API_KEY=sk-your-api-key-here

# CORS Origins (production only)
# ALLOWED_ORIGINS=https://your-domain.com,https://another-domain.com

# Security Settings
# SESSION_SECRET=your-session-secret-here

# Database Configuration (if needed in future)
# DATABASE_URL=postgresql://username:password@localhost:5432/ozwel_ai

# Logging Configuration
LOG_LEVEL=info

# Feature Flags
ENABLE_API_PROXY=true
ENABLE_HEALTH_CHECK=true
ENABLE_DEMO_MODE=true
